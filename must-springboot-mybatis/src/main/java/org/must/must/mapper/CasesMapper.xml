<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.must.must.mapper.CasesMapper">
	<select id="getAllCase" resultType="org.must.must.model.Cases">
        select * from CASE;
    </select>
    <select id="getCaseByCase" resultType="org.must.must.model.Cases">
    	SELECT * FROM [dbo].[CASE]
		WHERE CASE_NO =#{CASE_NO};         
    </select>
    <select id="getCaseByCaseNo" resultType="org.must.must.model.Cases">
    	SELECT * FROM [dbo].[CASE]
		WHERE CASE_NO =#{CASE_NO};       
    </select>
    
</mapper>
